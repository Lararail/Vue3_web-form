V;;AACA,WAAK,aAAL,GAAqB,KAAK,QAAL,CAAc,MAAd,GAAuB,CAAvB,GACrB,EADqB,GAClB,+BADH;;AAEA,UAAG,CAAC,KAAK,aAAT,EAAuB;AACnB,QAAA,OAAO,CAAC,GAAR,CAAY,SAAZ,EAAsB,KAAK,KAA3B;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,YAAZ,EAAyB,KAAK,QAA9B;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ,EAAqB,KAAK,IAA1B;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,iBAAZ,EAA8B,KAAK,KAAnC;AACJ;AAEJ;AA5BI;AAbG,CAAf","sourcesContent":["<template>\n  <form @submit.prevent=\"handleSubmit\"> <!---prevent submitボタンを押しても記録を残す-->\n      <label>Email:</label>\n      <input type=\"email\" required v-model=\"email\">\n\n      <label>Password:</label>\n      <input type=\"password\" required v-model=\"password\">\n      <div v-if=\"passwordError\" class=\"error\">{{ passwordError }}</div>\n\n      <label>Role:</label>\n      <select v-model=\"role\">\n          <option value=\"developer\">Web Developer</option>\n          <option value=\"designer\">Web Designer</option>\n      </select>\n\n    <label>Skills: </label>\n    <input type=\"text\" v-model=\"tempSkill\" @keyup=\"addSkill\"><!--@keyup.alt=\"addskill\"-->\n    <div v-for=\"skill in skills\" :key=\"skill\" class=\"pill\"><!--:key=\"skill\"  ユニークなもの-->\n        <span @click=\"deleteSkill(skill)\">{{ skill }}</span>\n    </div>\n\n    <div class=\"term\">\n        <input type=\"checkbox\" required v-model=\"terms\">\n        <label>Accept terms and conditions</label>\n    </div>\n    <!-- <div>\n        <input type=\"checkbox\" value=\"yoshi\" v-model=\"names\">\n        <label>yoshi</label>\n    </div>\n    <div>\n        <input type=\"checkbox\" value=\"mario\" v-model=\"names\">\n        <label>mario</label>\n    </div>\n    <div>    \n        <input type=\"checkbox\" value=\"misuzu\" v-model=\"names\">\n        <label>misuzu</label>\n    </div> -->\n\n    <div class=\"submit\">\n        <button>Create an account</button>\n    </div>\n  </form>\n    <p>Email: {{ email }}</p>\n    <p>Password: {{ password }}</p>\n    <p>Role: {{ role }}</p>\n    <p>Terms accepted: {{ terms }}</p>\n    <!-- <p>names: {{ names }}</p> -->\n    <!-- <p>Skills: {{ skills }}</p> -->\n</template>\n\n<script>\nexport default {\n    data(){\n        return{\n            email: '',\n            password: '',\n            role: '',\n            terms: false,\n            //names: []\n            tempSkill:'',\n            skills: [],\n            passwordError:''\n        }\n    },\n    methods:{\n        addSkill(e){\n            //console.log(e)\n            if(e.key === ',' && this.tempSkill){\n                if(!this.skills.includes(this.tempSkill)){\n                    this.skills.push(this.tempSkill)\n                }\n            this.tempSkill = ''\n            }\n            this.skill.slice( 0, -1 ) \n        },\n        deleteSkill(skill){\n            this.skills = this.skills.filter((item)=>{  //filter if false then remove item from array\n                return skill !== item\n            })\n        },\n        handleSubmit(){\n            console.log('submit')\n            //validate password\n            this.passwordError = this.password.length > 5 ? \n            '':'password must be 6 chars long'\n            if(!this.passwordError){\n                console.log('email: ',this.email)\n                console.log('password: ',this.password)\n                console.log('role: ',this.role)\n                console.log('term accepted: ',this.terms)\n            }\n\n        }\n    }\n}\n</script>\n\n<style>\nform{\n    max-width:420px;\n    margin:30px auto;\n    background:white;\n    text-align:left;\n    padding:40px;\n    border-radius:10px;\n}\nlabel{\n    color:#aaa;\n    display:inline-block;\n    margin:25px 0 25px;\n    font-size:0.6em;\n    text-transform:uppercase;\n    letter-spacing:1px;\n    font-weight:bold;\n}\ninput, select{\n    display:block;\n    padding:10px 6px;\n    width:100%;\n    box-sizing:border-box;\n    border:none;\n    border-bottom:1px solid #ddd;\n    color:#555;\n}\ninput[type=\"checkbox\"]{\n    display:inline-block;\n    width:16px;\n    margin:0 10px 0 0;\n    position:relative;\n    top:2px;\n}\n.pill{\n    display:inline-block;\n    margin:20px 10px 0 0;\n    padding:6px 12px;\n    background:#eee;\n    border-radius:20px;\n    font-size:12px;\n    letter-spacing:1px;\n    font-weight:bold;\n    color:#777;\n    cursor:pointer;\n}\nbutton{\n    background:#0b6dff;\n    border:0px;\n    padding:10px 20px;\n    margin-top:20px;\n    color:white;\n    border-radius:20px;\n}\n.submit{\n    text-align:center;\n}\n.error{\n    color:#ff0062;\n    margin-top:20px;\n    font-size:0.8em;\n    font-weight:bold;\n}\n</style>"],"sourceRoot":""}]}